{
  "name": "reactor-ui",
  "version": "0.0.18",
  "description": "Reactor UI Kit",
  "main": "lib",
  "scripts": {
    "build-npm": "babel -d dist/ ./src/js && cp README.md dist/ && cp package.json dist/",
    "dev": "webpack-dev-server --port 8081 --config webpack.dev.config.js --watch ",
    "test": "mocha --require should --require test/setup --recursive --compilers js:babel-register",
    "test-debug": "iron-node node_modules/mocha/bin/_mocha --require should --require test/setup --recursive --compilers js:babel-register ",
    "lint": "eslint src/* test/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wmira/reactor-ui"
  },
  "keywords": [
    "react",
    "react-component",
    "react ui",
    "vertical menu",
    "side menu",
    "panel",
    "button",
    "react widgets",
    "widget"
  ],
  "author": "Warren Mira warrenmira@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-webpack-alias": "^1.3.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.9.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^2.1.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.7.0",
    "eslint-plugin-mocha": "^3.0.0",
    "eslint-plugin-react": "^3.5.1",
    "jsdom": "^9.4.0",
    "mocha": "^2.3.3",
    "react-addons-test-utils": "^15.2.0",
    "should": "^9.0.2",
    "sinon": "^1.17.4",
    "webpack": "^1.13.1",
    "react": "^15.2.0",
    "react-dom": "^15.2.0"
  },
  "peerDependencies": {
    "react": "^15.2.0",
    "react-dom": "^15.2.0"
  }
}
